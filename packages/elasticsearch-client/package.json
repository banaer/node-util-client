  "main": "dist/index.js",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "lint": "bash ./scripts/lint-staged-files.sh",
